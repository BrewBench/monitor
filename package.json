{
  "name": "brewbench-monitor",
  "version": "3.3.0",
  "sketch_version": "3.3.0",
  "description": "Is an Arduino Yun brew monitor for the home brewer enthusiast that uses the Arduino REST API to interface with thermistors connected to the analog ports.",
  "main": "build/app.js",
  "scripts": {
    "test": "webpack && karma start karma.conf.js --single-run",
    "dev": "webpack-dev-server --progress --watch --inline --content-base ./build",
    "start": "webpack && npm run dev",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached --display-entrypoints"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brewbench/monitor.git"
  },
  "keywords": [
    "Arduino",
    "brewing",
    "thermistor",
    "temperature",
    "controller",
    "monitor",
    "homebrew",
    "remote",
    "Ranco",
    "PT100",
    "DS18B20",
    "DHT11",
    "DHT12",
    "DHT21",
    "DHT22",
    "DHT33",
    "DHT44",
    "TP-Link"
  ],
  "author": {
    "name": "Andrew Van Tassel",
    "email": "andrew@andrewvantassel.com",
    "url": "https://andrewvantassel.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brewbench/monitor/issues"
  },
  "homepage": "https://brewbench.co",
  "devDependencies": {
    "angular-mocks": "1.6.6",
