    "babel-preset-es2015": "^6.24.1",
    "browser-sync": "^2.21.0",
    "browser-sync-webpack-plugin": "^1.2.0",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-loader": "^0.5.1",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-spec-reporter": "^0.0.31",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
